{
  "name": "@jabuco/plugnplay",
  "version": "3.3.1",
  "description": "Plugin system for reusable code in node.js",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/e0ipso/plugnplay.git"
  },
  "author": {
    "name": "Mateu (e0ipso)",
    "id": 1140906,
    "url": "https://github.com/e0ipso",
    "email": "mateu.aguilo.bosch@gmail.com"
  },
  "license": "GPL-2.0",
  "engines": {
    "node": ">=6.0.0"
  },
  "private": false,
  "bugs": {
    "url": "https://github.com/e0ipso/plugnplay/issues"
  },
  "homepage": "https://github.com/e0ipso/plugnplay#readme",
  "scripts": {
    "coverage": "istanbul check-coverage --statements 100 --lines 100 --branches 100 --functions 100 **/coverage.json",
    "test": "npm run build && npm run istanbul",
    "istanbul": "istanbul cover --print both node_modules/nodeunit/bin/nodeunit ./test",
    "commitmsg": "validate-commit-msg",
    "precommit": "npm run flow && npm run lint",
    "prepush": "npm run test && npm run coverage",
    "semantic-release": "semantic-release",
    "postinstall": "npm run build",
    "flow": "flow",
    "build": "npm run clean && npm run build:flow && npm run build:copy && npm run build:flow",
    "build:flow": "flow-copy-source src lib",
    "build:copy": "cpx ./src/**/plugnplay.yml lib",
    "clean": "npm run clean:lib && npm run clean:flow",
    "clean:lib": "rimraf lib",
    "clean:flow": "flow-remove-types --all src/ --out-dir lib/",
    "lint": "eslint src",
    "emdaer": "emdaer"
  },
  "keywords": [
    "plugin",
    "polymorphism"
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cpx": "^1.5.0",
    "emdaer": "^0.3.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-flowtype": "^3.10.3",
    "eslint-plugin-import": "^2.17.3",
    "flow-bin": "^0.74.0",
    "husky": "^2.4.1",
    "istanbul": "^0.4.5",
    "nodeunit": "^0.11.3",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.16",
    "semantic-release-conventional-commits": "^2.0.1",
    "sinon": "^5.1.1",
    "validate-commit-msg": "^2.14.0"
  },
  "dependencies": {
    "circular-json": "^0.5.9",
    "copy": "^0.3.2",
    "easy-factory": "^1.3.0",
    "flow-copy-source": "^2.0.6",
    "flow-remove-types": "^2.101.0",
    "glob": "^7.1.4",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "marked": "^0.4.0",
    "md5": "^2.2.1",
    "pify": "^3.0.0",
    "require-subvert": "^0.1.0"
  },
  "release": {
    "analyzeCommits": "semantic-release-conventional-commits"
  }
}
